{
  "name": "next-cs-forms",
  "description": "Forms for CS based on next",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "engines": {
    "node": ">=7.6.0"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "setup": "npm install",
    "standard-fix": "standard --fix",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "now-deploy": "npm test && now alias"
  },
  "keywords": [
    "forms",
    "next",
    "cs",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/next-cs-forms.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/next-cs-forms/issues"
  },
  "now": {
    "alias": [
      "forms.cs.next.t-fk.win",
      "cs.next.t-fk.no"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm"
  },
  "devDependencies": {
    "ava": "1.0.1",
    "coveralls": "3.0.2",
    "nsp": "3.2.1",
    "nyc": "13.1.0",
    "standard": "12.0.1"
  },
  "dependencies": {
    "axios": "0.18.0",
    "bluebird": "3.5.3",
    "jws": "3.1.5",
    "muicss": "0.9.41",
    "next": "7.0.2",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "simple-encryptor": "2.0.0"
  }
