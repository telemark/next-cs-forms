{
  "name": "next-cs-forms",
  "description": "Forms for CS based on next",
  "version": "1.0.1",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "engines": {
    "node": ">=10.14.2"
  },
  "scripts": {
    "test": "standard && npm audit && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "setup": "npm install",
    "standard-fix": "standard --fix",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "now-deploy": "npm test && now alias",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "forms",
    "next",
    "cs",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/next-cs-forms.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/next-cs-forms/issues"
  },
  "now": {
    "alias": [
      "forms.cs.next.t-fk.win",
      "cs.next.t-fk.no"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm"
  },
  "devDependencies": {
    "ava": "1.4.1",
    "babel-eslint": "10.0.2",
    "coveralls": "3.0.3",
    "nyc": "14.1.1",
    "standard": "12.0.1"
  },
  "dependencies": {
    "axios": "0.18.0",
    "bluebird": "3.5.4",
    "jws": "3.2.2",
    "muicss": "0.9.41",
    "next": "8.1.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "simple-encryptor": "3.0.0"
  },
  "standard": {
    "ignore": [
      "/static/*.js"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    }
  }
}
